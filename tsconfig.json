{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "paths": {
      "@components/*": [
        "./src/components/*"
      ],
      "@navigation/*": [
        "./src/navigation/*"
      ],
      "@screens/*": [
        "./src/screens/*"
      ],
      "@hooks/*": [
        "./src/hooks/*"
      ],
      "@context/*": [
        "./src/context/*"
      ],
      "@assets/*": [
        "./src/assets/*"
      ],
      "@shared/*": [
        "./src/shared/*"
      ]
    },
    "baseUrl": ".",
    "typeRoots": [
      "./src/types"
    ],
    "reactNamespace": "React"
    // "types": [
    //   "./src/types"
    // ],
  },
}
